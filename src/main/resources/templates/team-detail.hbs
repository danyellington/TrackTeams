{{#partial "content"}}

{{#if team}}
<h5>Team:</h5>
<p>{{ team.teamName }}</p>
<h5>Team Description:</h5>
<p> {{ team.description }}</p>
<h5>Team Members:</h5>
<p> {{ team.memberName }}</p>
{{#if team.published }}

<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Draft
{{/if}}
<a href="/teams/{{team.id}}/update">Edit this team</a><br>
{{else}}
<p>( ͡° ͜ʖ ͡°)</p>
{{/if}}


{{/partial}}

{{> layout.hbs}}